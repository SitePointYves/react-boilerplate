{
  "name": "react-boilerplate",
  "version": "0.0.5",
  "private": true,
  "main": "src/main/main.js",
  "author": "Yves Kircher",
  "description": "Electron + NextJS example project",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "report:coverage": "nyc report --reporter=html",
    "coverage:verify": "npx nyc report --check-coverage true --lines 80",
    "electron:start": "electron .",
    "electron:dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "electron:build": "next build && electron-builder",
    "sonar": "sonar-scanner",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-data-grid": "^6.20.0",
    "@next/bundle-analyzer": "^14.2.3",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "electron-serve": "^2.0.0",
    "fontsource-roboto": "^4.0.0",
    "http-status-codes": "^2.3.0",
    "i18next": "^23.11.5",
    "next": "14.2.3",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^8.1.0",
    "ni18n": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-i18next": "^14.1.2",
    "react-redux": "^9.1.2",
    "redux-saga": "^1.3.0",
    "swc-plugin-coverage-instrument": "^0.0.21",
    "ts-node": "^10.9.2",
    "use-client": "^1.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.39",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.10.0",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^29.1.3",
    "typescript": "^5.4.5"
  }
}
